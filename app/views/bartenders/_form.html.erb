  
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
<div class="col-xs-1" align="center">
<div id="signup-form" class="hide">
<div class="container">
<%= form_with(model: @bartender) do |b| %>
    <fieldset>
<div class="form-group">
    <div class="col-md-4 inputGroupContainer">
        <div class="input-group">
        <%= b.label :username, class: "form-control" %> 
        <%= b.text_field :username, class: "form_input" %>
            <%# <input name= b.username placeholder="Username" class="form-control" > %>
        </div>
    </div>
</div>


<div class="form-group">
    <div class="col-md-4 inputGroupContainer">
        <div class="input-group">
            <input name= b.about placeholder="About" class="form-control" >
        </div>
    </div>
</div>

<div class="form-group">
    <div class="col-md-4 inputGroupContainer">
        <div class="input-group">
             <input name= b.email placeholder="Email" class="form-control" >
        </div>
    </div>
</div>

<div class="form-group">
    <div class="col-md-4 inputGroupContainer">
        <div class="input-group">
            <input name= b.password placeholder="Password" class="form-control"  type="password">
        </div>
    </div>
</div>

<div class="form-group"> 
    <div class="col-md-4 inputGroupContainer">
        <div class="input-group">
            <input name="confirm_password" placeholder="Confirm Password" class="form-control"  type="password">
        </div>
    </div>
</div>

<div class="form-group">
  <label class="col-md-4 control-label"></label>
  <div class="col-md-4"><br>
    <button type="submit" class="btn btn-success" >SUBMIT</button>
  </div>
</div>

</fieldset>
</form>
</div>
</div>
</div>
<%end%>