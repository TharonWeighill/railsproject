<body>
    <div class="login"> 
                <%= form_with(model: @bartender, url: "/login") do |b| %>
            <%= b.label :username %>
            <%= b.text_field :username %>
            <%= b.label :password %>
            <%= b.password_field :password %>
            <%= b.submit :login, class: "btn btn-primary btn-block btn-large"%>
        <% end %>
       
        <%= button_to "Sign Up", new_bartender_path, class:"btn btn-primary btn-block btn-large", method: :get%>
        <%= button_to "Login with Google", '/auth/google_oauth2', class:"btn btn-primary btn-block btn-large" %>
        </form>
    </div>
    </div>
</body>

